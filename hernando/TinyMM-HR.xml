<?xml version="1.0" encoding="utf-8"?>
<Containers>
<Beta>False</Beta>
<Category>MediaManager</Category>
  <Name>TinyMM</Name>
  <Description>XYZ

    

  </Description>
  <Overview>
   Tiny Media Manager. 
  </Overview>
 
  <Registry>https://registry.hub.docker.com/u/romancing/tinymediamanager</Registry>
  <Repository>romancin/tinymediamanager:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5800</HostPort>
        <ContainerPort>5800</ContainerPort>
        <Protocol></Protocol>
      </Port>
	    <Port>
        <HostPort>5900</HostPort>
        <ContainerPort>5900</ContainerPort>
        <Protocol></Protocol>
      </Port>
	  
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>USER_ID</Name>
      <Value>0</Value>
    </Variable>
	
    <Variable>
      <Name>TZ</Name>
      <Value>America/NewYork</Value>
    </Variable>
	
	<Variable>
      <Name>DISPLAY_HEIGHT</Name>
      <Value>1480</Value>
    </Variable>

    <Variable>
      <Name>DISPLAY_WIDTH</Name>
      <Value>1600</Value>
    </Variable>	
	
  </Environment>    
  <Data>
    <Volume>
      <HostDir>/mnt/user/Media/</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
     <Volume>
      <HostDir>/mnt/cache/appdata/tmm/config/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 

  </Data>  
  <WebUI>http://[IP]:[PORT:5800]</WebUI>
  <Banner>https://i.imgur.com/CU1cNL6.png</Banner>
  <Icon>https://i.imgur.com/CU1cNL6.png</Icon>
</Containers>
